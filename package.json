{
  "name": "advent-of-code-2022",
  "version": "1.0.0",
  "description": "RayJSeth's Advent of Code 2022 repo",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "npx tsc && cp -R ./src/inputs ./dist",
    "clean-build": "npm run clean && npm run build",
    "start": "node dist/index.js",
    "debug": "npm run clean-build && npm run start",
    "prettier": "prettier --check src/**/*.ts",
    "prettier:fix": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "AoC",
    "AdventOfCode",
    "Advent"
  ],
  "author": "Seth R Johnson (He/Him)",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.10",
    "prettier": "^2.8.0",
    "typescript": "^4.9.3"
  }
}
